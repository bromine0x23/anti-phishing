%footer.footer.footer-default.footer-static-bottom
	.container
		= image_tag 'logo.svg', class: 'brand-logo pull-right', alt: translate('cmb')
		%span.align-middle.text-muted
			- chrome = link_to 'support/Chrome.exe' do
				%span.fa.fa-chrome
					= translate 'chrome'
			- firefox = link_to 'support/Firefox.exe' do
				%span.fa.fa-firefox
					= translate 'firefox'
			!= translate('sessions.new.footer') % { chrome: chrome, firefox: firefox }