= render 'frames/reports/create_modal'
= render 'frames/reports/show_modal'
.wrapper
	.content-wrapper
		- has_permission 'on_duty' do
			%section.content
				.row
					.col-md-12
						.box
							.box-body
								.progress
									#progress-bar.progress-bar.progress-bar-warning.progress-bar-striped.active{ role: 'progressbar', 'aria-valuenow': 120, 'aria-valuemin': 0, 'aria-valuemax': 120 }
								= render 'frames/reports/index_form'
						.box
							.box-body
								= render 'frames/reports/index_table'
		- no_permission 'on_duty' do
			%section.content
				.row
					.col-md-12
						.box.box-warning.box-solid
							.box-header.with-border
								%h3.box-title
									错误
							.box-body
								你没有浏览权限